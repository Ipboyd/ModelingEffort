% IC Input

trial = 1;
label = 'E'; % E or I label
ICdir = '';
locNum = 1;
ICinput = genICinput(trial,ICdir,locNum,label);

g_postIC = 0.1
E_exc = 0
tauR = 1
tauD = 3
netcon=eye(N_pre,N_post)

iIC(X,t) = g_postIC .* ICinput(ceil((t+dt*0.01)/dt),:) * netcon .* (X - E_exc);

% monitor functions
monitor iIC

@isyn += iIC(X,t)

% IC Input
% Jio Nocon, September 2021

%%%%%%%% default parameters %%%%%%%%%%
trial = 1
locNum = []
label = 'E'

%%%%%%%% network parameters %%%%%%%%%%
g_postIC=0.1
E_exc=0
netcon=eye(N_pre,N_post)

% load input spikes
input = genPoissonInputs(trial,locNum,label)

iIC(X) = g_postIC * (input(k,:) * netcon) .* (X - E_exc);

% monitor functions
monitor iIC

@isyn += iIC(X)

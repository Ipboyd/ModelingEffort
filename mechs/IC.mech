% IC Input
% Jio Nocon, September 2021

%%%%%%%% default parameters %%%%%%%%%%
trial = 1;
locNum = [];

%%%%%%%% network parameters %%%%%%%%%%
g_postIC = 0.1
E_exc = 0
netcon = eye(N_pre,N_post)

%%%%%%%% network parameters %%%%%%%%%%
ICinput = genICinput(trial,[],locNum);
iIC(X,t) = g_postIC .* ICinput(k,:) * netcon .* (X - E_exc);

% monitor functions
monitor iIC

@isyn += iIC(X,t)
